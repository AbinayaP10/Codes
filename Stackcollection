import java.util.*;
class main{
    public static void main(String[]args){
        Scanner sc=new Scanner(System.in);
        Stack<Integer> stack=new Stack<>();
        while(true)
        {
            int ch=sc.nextInt();
            if(ch==-1)
                break;
            switch(ch){
                case 1:
                    int val=sc.nextInt();
                    stack.push(val);
                    break;
                case 2:
                    stack.pop();
                    break;
                case 3:
                    System.out.println(stack);
                    break;
                case 4:
                    System.out.println(stack.peek());
                    break;

            }
        }
    }
}
