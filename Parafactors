import java.util.*;
class Main {
    public static void main(String[] args) {
        Scanner obj = new Scanner(System.in);
        long num = obj.nextInt();
        long i;
        for(i=1;i*i<num;i++) {
            if(num%i==0)
                System.out.printf("(%d * %d)\n",i,num/i);
        }
        if(i*i==num)
            System.out.print(i);
    }
    }
